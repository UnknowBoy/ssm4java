  <select id="getAllUserAndRoleAndMenu" resultMap="userRoleList">
	select u.*,r.*,m.* from ERP_USER u
	join ERP_ROLE_USER ru on ru.user_no=u.user_no
	join ERP_ROLE r on r.role_no=ru.role_no
	join ERP_ROLE_MENU rm on rm.role_no=r.role_no
